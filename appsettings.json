{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://your-hostname:443",
        "Certificate": {
          "Store": "WebHosting",
          "Location": "LocalMachine",
          "Subject": "your-hostname"
        }
      }
    }
  },
  "ActionableMessages": {
		"CallbackBaseUrl": "https://your-hostname",
		"OriginatorIds": [
			"YOUR-ORIGINATOR-ID",
			"ANOTHER-ORIGINATOR-ID"
		],
    "EntraTenantId": "YOUR-ENTRA-TENANT-ID",
    "EntraClientId": "YOUR-ENTRA-CLIENT-ID",
    "EntraAudience": "api://YOUR-ENTRA-CLIENT-ID",
    "AdditionalAudiences": [
      "https://your-hostname"
    ],
    "EntraAuthorityHost": "https://login.microsoftonline.com"
  }
}
